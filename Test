{
	localStorage.setItem("handlecollection", "dep-nam");
	handlecollection='dep-nam' 
}
else if(window.location.href.includes('-nu'))
{
	localStorage.setItem("handlecollection", "hunter-nu");
	handlecollection='hunter-nu'
}
	else if(window.location.href.includes('-be-gai'))
{
	localStorage.setItem("handlecollection", "giay-the-thao-be-gai");
	handlecollection='giay-the-thao-be-gai'
}
else if(window.location.href.includes('-be-trai'))
{
	localStorage.setItem("handlecollection", "giay-the-thao-be-trai");
	handlecollection='giay-the-thao-be-trai'
}
	else if(localStorage.getItem("handlecollection"))
	{
		handlecollection=localStorage.getItem("handlecollection")
	}
	$.get('/collections/'+handlecollection+'?view=flashpop',function(reponse){
		$('body').append(reponse);
	})
</script>
		
